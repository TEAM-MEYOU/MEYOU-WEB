{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@constants": "./src/constants",
          "@stores": "./src/stores",
          "@utils": "./src/utils",
          "@hooks": "./src/hooks",
          "@components": "./src/components"
        }
      },
      "@emotion/babel-plugin"
    ]
  ]
}
